{
  "ignore": [
    "node_modules/**"
  ],
  "env": {
    "development": {
      "presets": [
        ["env", {
          "targets": {
            "browsers": ["last 2 versions", "safari >= 7"]
          },
          "modules": false
        }],
        "react"
      ]
    },
    "production": {
      "presets": [
        ["env", {
          "targets": {
            "browsers": ["last 2 versions", "safari >= 7"]
          },
          "modules": false
        }],
        "react"
      ],
      "plugins": [
        "external-helpers"
      ]
    }
  }
}
